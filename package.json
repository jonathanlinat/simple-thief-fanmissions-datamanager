{
  "name": "simple-thief-fanmissions-datascraper",
  "description": "A very simple and automated tool to scrape Fan Missions data from multiple sources at once",
  "author": "Jonathan Linat <jonathan.linat@gmail.com>",
  "version": "0.1.0",
  "license": "MIT",
  "private": true,
  "main": "src/index.js",
  "scripts": {
    "git:commit": "lint-staged",
    "lint": "eslint ./src --fix",
    "scrape": "node src/index.js",
    "scrape:watch": "nodemon --trace-warnings src/index.js"
  },
  "dependencies": {
    "cheerio": "1.0.0-rc.12",
    "deepmerge": "4.3.1",
    "https": "1.0.0",
    "joi": "17.9.2",
    "module-alias": "2.2.3",
    "node-fetch": "3.3.1",
    "uuid": "9.0.0",
    "with-query": "1.3.0"
  },
  "devDependencies": {
    "@commitlint/cli": "17.6.6",
    "@commitlint/config-conventional": "17.6.6",
    "eslint": "8.44.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-config-prettier-standard": "4.0.1",
    "eslint-config-standard": "17.1.0",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-n": "16.0.1",
    "eslint-plugin-prettier": "5.0.0",
    "eslint-plugin-promise": "6.1.1",
    "lint-staged": "13.2.3",
    "nodemon": "3.0.1",
    "prettier": "3.0.0",
    "prettier-config-standard": "5.0.0",
    "simple-git-hooks": "2.8.1"
  },
  "_moduleAliases": {
    "@modules": "./src/modules",
    "@recipes": "./src/recipes",
    "@shared": "./src/_shared"
  }
}
